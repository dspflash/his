<GroupBox Name="GBDEDITmedication" text="Search" toggle="1"><Ctrls>
<GroupBox Name="GB_medication_no" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_medication_no" Size="60,20" Text="medication_no" Align="Right"/>
<TextBox Name="medication_no" Size="170,20" Text="" Weight="170" ToolTip="ttp_medication_no"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_dispensaryid" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_dispensaryid" Size="60,20" Text="dispensaryid" Align="Right"  onload="init"/>
<ComboBox Name="dispensaryid" Size="150,100" Text="" Weight="150"  ToolTip="ttp_dispensaryid"  onload="init">
	<DbDetails>
<Index>dispensaryid</Index>
<DbTable>his.dispensary</DbTable>
<Data>dispensaryname,dispensaryid</Data>
<Where></Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
<MagicCtrl Name="refreshdispensaryid" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="dispensaryid"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
<GroupBox Name="GB_dept_category" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_dept_category" Size="60,20" Text="dept_category" Align="Right"  onload="init"/>
<ComboBox Name="dept_category" Size="150,100" Text="" Weight="150"  ToolTip="ttp_dept_category"  onload="init">
	<DbDetails>
<Index>dept_categoryid</Index>
<DbTable>his.dept_category</DbTable>
<Data>dept_categoryname,dept_categoryid</Data>
<Where></Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
<MagicCtrl Name="refreshdept_category" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="dept_category"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_doctor_no" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
