<GroupBox cols="xs12 sm6 md4" Name="GBOPVbottle_transaction" Size="100,100"><Ctrls>
<Button Text="delete" ToolTip="ttp_deletebottle_transaction" Name="deletebottle_transaction" Size="50,20">
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>his.bottle_transaction</DbTable>
<Where>bottle_transactionid='%{bottle_transactionLst[bottle_transactionid]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchbottle_transaction" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Text="refresh" ToolTip="ttp_refreshbottle_transaction" Name="searchbottle_transaction" Size="50,20">
			  <OnClick><submit next="bottle_transactionLst"/>	</OnClick>
</Button>
<TAB/><TAB/><Button Text="firstpage" Name="FPbottle_transactionLst" OnClick="(bottle_transactionLst)GetPage(First)" Size="50,20"/>
<TAB/><Button Text="prepage" Name="PREbottle_transactionLst" OnClick="(bottle_transactionLst)GetPage(Pre)" Size="50,20"/>
<TAB/><Button Text="nextpage" Name="NPbottle_transactionLst" OnClick="(bottle_transactionLst)GetPage(Next)" Size="50,20"/>
</Ctrls></GroupBox>