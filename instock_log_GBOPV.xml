<GroupBox cols="xs12 sm6 md4" Name="GBOPVinstock_log" Size="100,100"><Ctrls>
<Button Text="delete" ToolTip="ttp_deleteinstock_log" Name="deleteinstock_log" Size="50,20">
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>his.instock_log</DbTable>
<Where>instockid='%{instock_logLst[instockid]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchinstock_log" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Text="refresh" ToolTip="ttp_refreshinstock_log" Name="searchinstock_log" Size="50,20">
			  <OnClick><submit next="instock_logLst"/>	</OnClick>
</Button>
<TAB/><TAB/><Button Text="firstpage" Name="FPinstock_logLst" OnClick="(instock_logLst)GetPage(First)" Size="50,20"/>
<TAB/><Button Text="prepage" Name="PREinstock_logLst" OnClick="(instock_logLst)GetPage(Pre)" Size="50,20"/>
<TAB/><Button Text="nextpage" Name="NPinstock_logLst" OnClick="(instock_logLst)GetPage(Next)" Size="50,20"/>
</Ctrls></GroupBox>