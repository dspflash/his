<GroupBox cols="xs12 sm6 md4" Name="GBOPEinstock_log" Size="100,100"><Ctrls>
<Button sqlpkinsert="instockid" Text="create" ToolTip="ttp_createinstock_log" Name="insertinstock_log" Size="50,20">
<OnClick>
<DbDetails mode="insert" fcsm="1">
<DbTable>his.instock_log</DbTable>
<Required>%{dataRequired}</Required>
<Requireless>%{dataRequireless}</Requireless>
<Data>medicineid[medicineid],medicineno,bottleid[bottleid],intype[statuscode],receiveempid[user_id],receiveemp,stockno[stockid],locationno[locationid],bottleno,manufacturer,weight,validday,memo</Data>
<DataEx>inserttime,opera;now(),'%{operaid}'</DataEx>
<queryFormat>insert into %{table} %{data}</queryFormat></DbDetails>
<submit namelist="DbDetails"/><if cond="!%{insertinstock_log[broke]}"><if cond="%{refreshAfterEdit}==1"><submit next="instock_logLst" delay="1000"/><elseif cond="%{refreshAfterEdit}==2"/><if cond="1"><if cond="'%{frmwndctrl}'"><submit next="%{frmwndctrl}" delay="1000"/><else/><submit next="$parent.instock_log_vLst" delay="1000"/></if></if></if></if>
	</OnClick>
	</Button><TAB/>
<Button Text="modify" ToolTip="ttp_modifyinstock_log" Name="modifyinstock_log" Size="50,20">
<OnClick>
<DbDetails mode="update" fcsm="1">
<DbTable>his.instock_log</DbTable>
<Required>%{dataRequired}</Required>
<Requireless>%{dataRequireless}</Requireless>
<Data>medicineid[medicineid],medicineno,bottleid[bottleid],intype[statuscode],receiveempid[user_id],receiveemp,stockno[stockid],locationno[locationid],bottleno,manufacturer,weight,validday,memo</Data>
<DataEx>opera,inemp;'%{operaid}','%{operaid}'</DataEx>
<Where>instockid='%{instockid}'</Where>
<queryFormat>update %{table} set %{data} where %{where}</queryFormat></DbDetails>
<submit namelist="DbDetails"/><if cond="!%{modifyinstock_log[broke]}"><if cond="%{refreshAfterEdit}==1"><submit next="instock_logLst" delay="1000"/><elseif cond="%{refreshAfterEdit}==2"/><if cond="'%{frmwndctrl}'"><submit next="%{frmwndctrl}" delay="1000"/><else/><submit next="$parent.instock_log_vLst" delay="1000"/></if></if></if>
	</OnClick>
</Button><TAB/>
<Button Text="reset" ToolTip="ttp_resetinstock_log" Name="resetinstock_log" Size="50,20">
<OnClick>
<submit next="GBDEDITinstock_log" function="reset"/>
	</OnClick>
</Button><TAB/>
<Button Text="Search" ToolTip="ttp_gsearchinstock_log" Name="gsearchinstock_log" Size="50,20" visible="false">
<OnClick>
<search next="instock_logLst" listed="0" prefixwhere="0" prefixand="0" inputed="1"/><submit next="GBDEDITinstock_log" namelist="search" function="search"/>
	</OnClick>
</Button><TAB/>
</Ctrls></GroupBox>