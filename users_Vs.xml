<GroupBox Name="GBDEDITusers" text="Search" toggle="1"><Ctrls>
<GroupBox Name="GB_user_name" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_user_name" Size="60,20" Text="user_name" Align="Right"/>
<TextBox Name="user_name" Size="170,20" Text="" Weight="170" ToolTip="ttp_user_name"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_user_role" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
