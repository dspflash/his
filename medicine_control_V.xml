<WndTap Name="medicine_control_v" Text="medicine_control_v">
<WndTaps>
<xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="medicine_control_Ve.xml" popup="1"/>
</WndTaps>
<var name="searchtmpl"><medicineid format="m.medicineid='%{opvalue}'"/><relation format="m.relation='%{opvalue}'"/><medicine2id format="m.medicine2id='%{opvalue}'"/><explains format="(m.explains like '%%{opvalue}%')"/><uptime format="(m.uptime between '%{uptime[datetime]}' and '%{uptime[datetime2]}')"/><opera format="m.opera='%{opvalue}'"/><controlid format="(m.controlid like '%%{opvalue}%')"/></var>
<xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="lan\medicine_control.xml"/>
<Ctrls><GroupBox  Name="GBmedicine_control" id="medicine_control" Text="medicine_control" Size="100,100"><Ctrls>
<xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="medicine_control_Vs.xml"/>
<BR AutoHeight="1"/><GroupBox  Name="GBOPVmedicine_control" Size="100,20" cols="xs12 sm6 md4"><Ctrls>
<Button Text="addone" ToolTip="ttp_addone" Name="addone" Size="50,20">	<OnClick><submit next="$this.medicine_control.GBDEDITmedicine_control" function="reset"/><assign name="$this.medicine_control.frmwndctrl" expr="%{$this.medicine_control_vLst}"/></OnClick>	</Button><TAB/>	
<Button Text="reset" ToolTip="ttp_resetmedicine_control" Name="resetmedicine_control" Size="50,20">	<OnClick>	<submit next="GBDEDITmedicine_control" function="reset"/>	</OnClick>	</Button><TAB/>	
<Button Text="Search" ToolTip="ttp_gsearchmedicine_control" Name="gsearchmedicine_control" Size="50,20">	<OnClick>	
<search next="medicine_control_vLst" listed="0" prefixwhere="0" prefixand="0" inputed="1">%{searchtmpl}</search><submit next="GBDEDITmedicine_control" namelist="search" function="search"/>	
<search next="records_medicine_control" listed="0" prefixwhere="1" prefixand="0"  inputed="1">%{searchtmpl}</search><submit next="GBDEDITmedicine_control" namelist="search" function="search"/>	
</OnClick></Button><TAB/></Ctrls></GroupBox>
<xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="medicine_control_Vpg.xml"/>
<xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="medicine_control_Vpgop.xml"/>
<BR AutoHeight="1"/><xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="medicine_control_Vv.xml"/>
</Ctrls></GroupBox>
</Ctrls></WndTap>
