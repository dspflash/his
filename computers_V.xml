</WndTaps>
<var name="searchtmpl"><computer_name format="(c.computer_name like '%%{opvalue}%')"/><computer_mac format="(c.computer_mac like '%%{opvalue}%')"/><computer_ip format="(c.computer_ip like '%%{opvalue}%')"/><memo format="(c.memo like '%%{opvalue}%')"/><delemp format="c.delemp='%{opvalue}'"/><deltime format="(c.deltime between '%{deltime[datetime]}' and '%{deltime[datetime2]}')"/><valid format="(c.valid like '%%{opvalue}%')"/><opera format="c.opera='%{opvalue}'"/><uptime format="(c.uptime between '%{uptime[datetime]}' and '%{uptime[datetime2]}')"/><computerid format="(c.computerid like '%%{opvalue}%')"/><dispensaryid format="(c.dispensaryid like '%%{opvalue}%')"/></var>
<xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="lan\computers.xml"/>
<Ctrls><GroupBox  Name="GBcomputers" id="computers" Text="computers" Size="100,100"><Ctrls>
<xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="computers_Vs.xml"/>
