<ComboBox Name="user_role" Size="150,100" Text="" Weight="150"  ToolTip="ttp_user_role"  onload="init">
	<DbDetails>
<Index>role_id</Index>
<DbTable>his.user_role</DbTable>
<Data>role_name,role_id</Data>
<Where></Where>
<queryFormat></queryFormat></DbDetails></ComboBox>
<MagicCtrl Name="refreshuser_role" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="user_role"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
<GroupBox Name="GB_real_name" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_real_name" Size="60,20" Text="real_name" Align="Right"/>
<TextBox Name="real_name" Size="170,20" Text="" Weight="170" ToolTip="ttp_real_name"></TextBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_dept_info" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_dept_info" Size="60,20" Text="dept_info" Align="Right"  onload="init"/>
<ComboBox Name="dept_info" Size="150,100" Text="" Weight="150"  ToolTip="ttp_dept_info"  onload="init">
	<DbDetails>
<Index>departmentid</Index>
<DbTable>his.department</DbTable>
