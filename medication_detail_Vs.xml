<TextBox Name="medicinenocn" Size="170,20" Text="" Weight="170" ToolTip="ttp_medicinenocn"></TextBox>
</Ctrls></GroupBox>
<GroupBox Name="GB_price" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_price" Size="60,20" Text="price" Align="Right"/>
