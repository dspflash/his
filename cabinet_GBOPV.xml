<GroupBox cols="xs12 sm6 md4" Name="GBOPVcabinet" Size="100,100"><Ctrls>
<Button Text="delete" ToolTip="ttp_deletecabinet" Name="deletecabinet" Size="50,20">
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>his.cabinet</DbTable>
<Where>cabinetid='%{cabinetLst[cabinetid]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchcabinet" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Text="refresh" ToolTip="ttp_refreshcabinet" Name="searchcabinet" Size="50,20">
			  <OnClick><submit next="cabinetLst"/>	</OnClick>
</Button>
<TAB/><TAB/><Button Text="firstpage" Name="FPcabinetLst" OnClick="(cabinetLst)GetPage(First)" Size="50,20"/>
<TAB/><Button Text="prepage" Name="PREcabinetLst" OnClick="(cabinetLst)GetPage(Pre)" Size="50,20"/>
<TAB/><Button Text="nextpage" Name="NPcabinetLst" OnClick="(cabinetLst)GetPage(Next)" Size="50,20"/>
</Ctrls></GroupBox>