<GroupBox Name="GBDEDITinstock_log"><Ctrls>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_medicineid"><Ctrls>
<Label Align="Right" Text="medicineid" onload="init" Name="lb_medicineid" Size="60,20"/>
<ComboBox Weight="150" Text="" ToolTip="ttp_medicineid" onload="init" Name="medicineid" Size="150,100">
	<DbDetails fcsm="true">
<Index>medicineid</Index>
<DbTable>his.medicine_info</DbTable>
<Data>medicinenocn,medicineno,medicineid</Data>
<Where/>
<queryFormat/></DbDetails><OnSelEndOk><submit next="bottleid"/></OnSelEndOk></ComboBox>
<MagicCtrl Name="refreshmedicineid" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="medicineid"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_medicineno"><Ctrls>
<Label Align="Right" Text="medicineno" Name="lb_medicineno" readonly="true" Size="60,20"/>
<TextBox Weight="170" Text="" ToolTip="ttp_medicineno" Name="medicineno" readonly="true" Size="170,20"/>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_bottleid"><Ctrls>
<Label Align="Right" Text="bottleid" Name="lb_bottleid" Size="60,20"/>
<ComboBox Weight="150" Text="" ToolTip="ttp_bottleid" Name="bottleid" Size="150,100">
	<DbDetails fcsm="true">
<Index>bottleid</Index>
<DbTable>his.bottle_info</DbTable>
<Data>bottleno,inqty weight,bottleid</Data>
<Where>medicineid='%{medicineid[medicineid]}' and uses_flag=100</Where>
<queryFormat/></DbDetails></ComboBox>
<MagicCtrl Name="refreshbottleid" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="bottleid"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_intype"><Ctrls>
<Label Align="Right" Text="intype" onload="init" Name="lb_intype" Size="60,20"/>
<ComboBox Weight="170" Text="" ToolTip="ttp_intype" onload="init" Name="intype" Size="170,100">
	<DbDetails>
<Index>statuscode</Index>
<DbTable>his.status_option</DbTable>
<Data>statusname,statuscode</Data>
<Where>statuscode in (101,102,106,107)</Where>
<queryFormat/></DbDetails></ComboBox>
</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_receiveempid"><Ctrls>
<Label Align="Right" Text="receiveempid" onload="init" Name="lb_receiveempid" Size="60,20"/>
<ComboBox Weight="150" Text="" ToolTip="ttp_receiveempid" onload="init" Name="receiveempid" Size="150,100">
	<DbDetails fcsm="true">
<Index>user_id</Index>
<DbTable>his.users</DbTable>
<Data>user_name,real_name receiveemp,user_id</Data>
<Where/>
<queryFormat/></DbDetails></ComboBox>
<MagicCtrl Name="refreshreceiveempid" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="receiveempid"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_receiveemp"><Ctrls>
<Label Align="Right" Text="receiveemp" Name="lb_receiveemp" readonly="true" Size="60,20"/>
<TextBox Weight="170" Text="" ToolTip="ttp_receiveemp" Name="receiveemp" readonly="true" Size="170,20"/>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_stockno"><Ctrls>
<Label Align="Right" Text="stockno" onload="init" Name="lb_stockno" Size="60,20"/>
<ComboBox Weight="150" Text="" ToolTip="ttp_stockno" onload="init" Name="stockno" Size="150,100">
	<DbDetails>
<Index>stockid</Index>
<DbTable>his.stockbaseinfo</DbTable>
<Data>stockname,stockid</Data>
<Where/>
<queryFormat/></DbDetails><OnSelEndOk><submit next="locationno"/></OnSelEndOk></ComboBox>
<MagicCtrl Name="refreshstockno" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="stockno"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
<GroupBox cols="xs12 sm6 md4" Multiline="0" Name="GB_locationno"><Ctrls>
<Label Align="Right" Text="locationno" Name="lb_locationno" Size="60,20"/>
<ComboBox Weight="150" Text="" ToolTip="ttp_locationno" Name="locationno" Size="150,100">
	<DbDetails>
<Index>locationid</Index>
<DbTable>his.stocklocationinfo</DbTable>
<Data>location_name,locationid</Data>
<Where>stockid='%{stockno[stockid]}'</Where>
<queryFormat/></DbDetails></ComboBox>
<MagicCtrl Name="refreshlocationno" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="locationno"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
