<TextBox Name="jiliang" Size="170,20" Text="" Weight="170" ToolTip="ttp_jiliang"><OnEnChange><assign name="xiaoji" expr="js(%{price}*%{jiliang})"/></OnEnChange></TextBox>
</Ctrls></GroupBox>
<BR AutoHeight="1"/><GroupBox Name="GB_xiaoji" Multiline="0" cols="xs12 sm6 md4"><Ctrls>
<Label Name="lb_xiaoji" Size="60,20" Text="xiaoji" Align="Right"/>
