<GroupBox Name="GBDVmedi_trans" cols="xs12 sm12 md12 lg12" visible="0"><Ctrls>
<ListCtrl Name="medi_transLst" ToolTip="ttp_medi_trans" Size="70,20" visible="0" loadedclick="1">
	<DbDetails fcsm="1">
<Index>transid</Index>
<DbTable>his.medi_trans</DbTable>
<Data>medicationid,medication_detailid,medicineid,bottleid,cabinetid,cabinet_cellid,equipmentid,equipment_posid,equipment_pos_degree,qtyapply,qtyrelease,opera,uptime,transid,weight1,weight2,status</Data>
<Order>transid</Order></DbDetails>
<OnClick><assign name="medicationid[medicationid]" expr="%{medi_transLst[medicationid]}" function="SetSel"/><assign name="medication_detailid[medication_detailid]" expr="%{medi_transLst[medication_detailid]}" function="SetSel"/><assign name="medicineid[medicineid]" expr="%{medi_transLst[medicineid]}" function="SetSel"/><assign name="bottleid[bottleid]" expr="%{medi_transLst[bottleid]}" function="SetSel"/><assign name="cabinetid[cabinetid]" expr="%{medi_transLst[cabinetid]}" function="SetSel"/><assign name="cabinet_cellid[cellid]" expr="%{medi_transLst[cabinet_cellid]}" function="SetSel"/><assign name="equipmentid[equipmentid]" expr="%{medi_transLst[equipmentid]}" function="SetSel"/><assign name="equipment_posid[equipmentid]" expr="%{medi_transLst[equipment_posid]}" function="SetSel"/><assign name="opera[user_id]" expr="%{medi_transLst[opera]}" function="SetSel"/>
<if cond="'%{transid}'"><SetVisible name="insertmedi_trans" Visible="0"/><SetVisible name="modifymedi_trans" Visible="1"/></if>
</OnClick>
</ListCtrl>
</Ctrls></GroupBox>